{
  "name": "elevate-site-content-client",
  "version": "0.1.0",
  "description": "Fetch and cache published site content via a cacheable GET endpoint.",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "@types/react": "^18.3.0"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT"
}
